<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SegundoParcial.RegisterPage"
             xmlns:viewmodel ="clr-namespace:SegundoParcial.ViewModel"
             x:DataType="viewmodel:RegisterViewModel"
             Title="RegisterPage">

    <Grid RowDefinitions="auto"
          ColumnDefinitions="auto"
          >
        <Label HorizontalTextAlignment="Center" TextColor="White" FontFamily="bold" BackgroundColor="LightCoral" Text="Registros"/>
        <CollectionView ItemsSource="{Binding Title}" SelectionMode="None">
            <CollectionView.ItemTemplate>
                <DataTemplate x:DataType="x:Type x:String">
                    
                </DataTemplate>
            </CollectionView.ItemTemplate>
        </CollectionView>
    </Grid>
</ContentPage>